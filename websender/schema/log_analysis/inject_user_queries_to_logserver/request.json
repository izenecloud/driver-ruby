{
  "title":"Inject User Queries To Log Server",
  "type":"map",
  "desc":"Inject user queries to log server.",
  "mapping": {
    "user_queries": {
      "type": "seq",
      "sequence": [{
        "type": "map",
        "mapping": {
          "query": {
            "type": "str",
            "required": true
          },

          "collection": {
            "type": "str",
            "required": true
          },

          "hit_docs_num": {
            "type": "int"
          },

          "page_start": {
            "type": "int"
          },

          "page_count": {
            "type": "int"
          },

          "duration": {
            "type": "str",
            "desc": "HH:MM:SS.fffffffff"
          },

          "timestamp": {
            "type": "str",
            "desc": "YYYY-mm-dd HH:MM:SS.fffffffff"
          }
        }
      }]
    }
  }
}
