{
  "title":"RecentKeywords",
  "type":"map",
  "desc":"Get keywords list.",
  "mapping": {
    "collection": {
      "type": "str",
      "desc": "Get keywords seached on specified collection",
      "required": true
    },

    "header": {
      "type":"map",
      "mapping": $(header)
    },

    "limit": {
      "type": "int",
      "desc": "Limit result count"
    },

    "select": {
      "type": "seq",
      "desc": "Select list type",
      "sequence": [{
        "type": "string",
        "title": "list type",
        "enum": ["recent", "popular", "realtime"]
      }]
    }
  }
}
