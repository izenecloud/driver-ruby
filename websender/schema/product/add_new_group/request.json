{
  "title":"add_new_group",
  "type":"map",
  "desc":"add new groups.",
  "mapping": {

    "header": {
      "type":"map",
      "mapping": $(header)
    },

    "collection": {
      "type": "str",
      "desc": "collection name",
      "required": true
    },

    "docid_list": {
      "type": "seq",
      "desc": "docid_list",
      "sequence": [{
        "type": "int",
        "title": "docid"
      }]
    },

    "resource": {
      "type":"map",
      "desc": "Object representing the docuemnt. Key is property key and value is property value.",
      "user_key": "property",
      "required": true,
      "mapping": {
        "property": {
          "type":"any",
          "desc":"Keys is property key, value is property value"
        }
      }
    },

    "option": {
      "type":"map",
      "desc": "Edit options.",
      "mapping": {
        "force": {
          "type":"bool",
          "desc":"If force to execute"
        }
      }
    }
  }
}
