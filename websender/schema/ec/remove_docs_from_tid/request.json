{
  "title":"remove_docs_from_tid",
  "type":"map",
  "desc":"remove documents id from specify tid.",
  "mapping": {

    "header": {
      "type":"map",
      "mapping": $(header)
    },

    "collection": {
      "type": "str",
      "desc": "collection name",
      "required": true
    },

    "tid": {
      "type": "int",
      "desc": "tid",
      "required": true
    },

    "docid_list": {
      "type": "seq",
      "desc": "docid_list",
      "sequence": [{
        "type": "int",
        "title": "docid"
      }]
    }
  }
}
