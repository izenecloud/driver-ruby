{
  "title":"AutoFill",
  "type":"map",
  "desc":"Get list of keywords starting with specified prefix.",
  "mapping": {
    "collection": {
      "type":"str",
      "desc":"Specify a collection name",
      "required":true
    },

    "prefix": {
      "type": "str",
      "required": true
    },

    "header": {
      "type":"map",
      "mapping": $(header)
    },

    "limit": {
      "type": "int",
      "desc": "Limit result count"
    }
  }
}
